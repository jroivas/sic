void tmp()
{
}

int first(int *arr)
{
    if (arr == NULL)
        return 0;

    int res = *arr;
    return res;
}

int last(int *arr)
{
    int last = 0;
    if (!arr)
        return last;
    while (*arr != 0) {
        last = *arr;
        arr++;
    }
    return last;
}

int cnt(int *arr)
{
    int cnt = 0;
    if (!arr)
        return cnt;
    while (*arr != 0) {
        ++arr;
        ++cnt;
    }

    return cnt;
}

int main()
{
    int n = 10;
    int val[n + 1];

    tmp();

    for (int i = 0; i < n; i++)
        val[i] = i + 2;

    val[n] = 0;
    
    if (first(val) != 2)
        return 1;

    if (last(val) != 11)
        return 2;

    int cnt_items = cnt(val);
    if (cnt_items != n)
        return 3;

    int *ptr = val;
    ptr += 2;
    if (*ptr != 4)
        return 4;

    ptr = ptr + 2;
    if (*ptr != 6)
        return 5;

    ptr = 1 + ptr;
    if (*ptr != 7)
        return 6;

    --ptr;
    if (*ptr != 6)
        return 7;

    ptr--;
    if (*ptr != 5)
        return 8;

    ptr -= 2;
    if (*ptr != 3)
        return 9;

    ptr = val;
    if (*ptr != 2)
        return 10;

    ptr = val + n - 1;
    if (*ptr != 11)
        return 11;

    return 0;
}

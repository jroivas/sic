int main()
{
    int a;
    int c;
    double b;

    a++;
    c = ++a;
    if (c != a)
        return -1;
    a--;
    a++;
    ++a;
    --a;
    c = a++;
    if (c == a)
        return -2;

    b++;
    b++;
    b--;
    ++b;
    b++;
    --b;
    b++;
    if (b != 3.0)
        return -3;

    return a;
}


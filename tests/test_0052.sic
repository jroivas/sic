
enum color {
    BLACK = 1,
    RED,
    BLUE,
    GREEN,
    WHITE = 10,
    GREY,
    BROWN
};

int main()
{
    enum color col;

    col = RED;
    if ((int)col != 2)
        return 1;

    col = BLUE;
    if ((int)col != 3)
        return 2;

    col = GREEN;
    if ((int)col != 4)
        return 3;

    col = BLACK;
    if ((int)col != 1)
        return 4;

    if ((int)BLUE != 3)
        return 5;

    if ((int)WHITE != 10)
        return 6;

    if ((int)GREY != 11)
        return 7;

    if ((int)BROWN != 12)
        return 8;

    return 0;
}

// Custom va_list instead of builtin
typedef struct {
    int a;
    int b;
    char *value;
} va_list;

void a(char *b)
{
}

int test(int cnt, ...)
{
    va_list tmp;
    int sum = 0;

    // Test that we can call any other method
    a(tmp);
    // Call directly builtin methods
    __builtin_va_start(tmp, cnt);
    for (int i = 0; i < cnt;  i++) {
        int val = __builtin_va_arg(tmp, int);
        sum += val;
    }

    __builtin_va_end(tmp);

    return sum;
}

int main()
{
    int sum = test(3, 1, 2, 4);
    if (sum != 7)
        return 1;
    return 0;
}
